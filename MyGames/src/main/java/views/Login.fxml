<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="854.0"
    xmlns="http://javafx.com/javafx/23.0.1"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="controllers.LoginController"
    stylesheets="/styles/LoginStyles.css"
    styleClass="background-pane">
    <children>
        <BorderPane AnchorPane.bottomAnchor="0.0"
            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
            AnchorPane.topAnchor="0.0">
            <top>
                <Pane prefHeight="30.0" BorderPane.alignment="CENTER" />
            </top>
            <left>
                <Pane prefWidth="235.0" BorderPane.alignment="CENTER" />
            </left>
            <bottom>
                <Pane prefHeight="30.0" BorderPane.alignment="CENTER" />
            </bottom>
            <right>
                <Pane prefWidth="235.0" BorderPane.alignment="CENTER" />
            </right>
            <center>
                <BorderPane maxHeight="419.2" maxWidth="384.0"
                    BorderPane.alignment="CENTER">
                    <center>
                        <BorderPane>
                            <top>
                                <GridPane prefHeight="32.0" prefWidth="384.0"
                                    BorderPane.alignment="CENTER">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES"
                                            minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints hgrow="SOMETIMES"
                                            minWidth="10.0" prefWidth="100.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0"
                                            prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <GridPane fx:id="paneBtnLogin"
                                            styleClass="btn-login-container">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES"
                                                    minWidth="10.0" prefWidth="100.0" />
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0"
                                                    prefHeight="30.0" vgrow="SOMETIMES" />
                                            </rowConstraints>
                                            <children>
                                                <Label alignment="CENTER" prefHeight="107.0"
                                                    prefWidth="192.0"
                                                    styleClass="btn-login"
                                                    text="LOGIN" textFill="WHITE">
                                                    <font>
                                                        <Font name="Consolas Bold" size="12.0" />
                                                    </font>
                                                </Label>
                                            </children>
                                        </GridPane>
                                        <GridPane fx:id="paneBtnRegister" alignment="CENTER"
                                            styleClass="btn-register-container"
                                            GridPane.columnIndex="1">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES"
                                                    minWidth="10.0" prefWidth="100.0" />
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0"
                                                    prefHeight="30.0" vgrow="SOMETIMES" />
                                            </rowConstraints>
                                            <children>
                                                <Label alignment="CENTER" prefHeight="121.0"
                                                    prefWidth="192.0"
                                                    styleClass="btn-register"
                                                    text="REGISTER" textAlignment="CENTER" textFill="WHITE">
                                                    <font>
                                                        <Font name="Consolas Bold" size="12.0" />
                                                    </font>
                                                </Label>
                                            </children>
                                        </GridPane>
                                    </children>
                                </GridPane>
                            </top>
                            <center>
                                <BorderPane fx:id="paneData" prefHeight="200.0"
                                    prefWidth="200.0" BorderPane.alignment="CENTER" />
                            </center>
                        </BorderPane>
                    </center>
                </BorderPane>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
